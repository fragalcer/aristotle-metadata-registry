  "scripts": {
    "build": "ASSET_PATH=$ASSET_PATH webpack --config webpack.prod.js",
    "devbuild": "webpack --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "test": "karma start karma.conf.js --single-run",
    "watchtest": "karma start karma.conf.js",
    "lint": "eslint --ext .js,.vue ./src",
    "installandwatch": "npm install && npm run watch"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "Firefox ESR",
    "IE 10"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/attributes-order": "off",
      "vue/order-in-components": "off"
    },
    "globals": {
      "$": true,
      "jQuery": true
    }
  },
