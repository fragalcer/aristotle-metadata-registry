  "browserslist": [
    "last 2 versions",
    "not dead",
    "Firefox ESR",
    "IE 10"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off"
